{
    "name": "vite-chakra",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "prettier": "prettier --write \"./{src,tests}/**/*\""
    },
    "dependencies": {
        "@chakra-ui/react": "^1.6.5",
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@pastable/core": "^0.1.15",
        "@xstate/react": "^1.6.3",
        "axios": "^0.24.0",
        "framer-motion": "^5.5.1",
        "jotai": "^1.4.7",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.22.1",
        "react-icons": "^4.3.1",
        "react-query": "^3.34.3",
        "react-router-dom": "^6.1.1",
        "xstate": "^4.26.1"
    },
    "devDependencies": {
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "prettier": "^2.5.1",
        "prettier-plugin-sorted": "^2.0.0",
        "typescript": "^4.5.4",
        "vite": "^2.7.2",
        "vite-plugin-pwa": "^0.11.11",
        "vite-react-jsx": "^1.1.2"
    },
    "importSort": {
        ".js, jsx, .ts, .tsx": {
